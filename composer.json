{
  "name": "miaoxing/product",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Product Plugin",
  "keywords": [
    "miaoxing",
    "product"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.5",
    "miaoxing/category": "^0.5.5",
    "miaoxing/admin": "^0.8.0",
    "miaoxing/nav": "^0.1.27",
    "miaoxing/logistics": "^0.5.4",
    "miaoxing/ueditor": "^0.1.21"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Product\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Product\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/admin": "miaoxing/admin as 0.7.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.8.x-dev",
      "miaoxing/services": "miaoxing/services as 0.5.x-dev",
      "miaoxing/logistics": "miaoxing/logistics as 0.5.x-dev",
      "wei/wei": "twinh/wei as 0.12.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.2"
  }
}
