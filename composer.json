{
  "name": "miaoxing/product",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Product Plugin",
  "keywords": [
    "miaoxing",
    "product"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.8.1",
    "miaoxing/category": "^0.5.29",
    "miaoxing/admin": "^0.12.1",
    "miaoxing/nav": "^0.1.49",
    "miaoxing/logistics": "^0.5.28",
    "miaoxing/ueditor": "^0.1.43"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Product\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Product\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/admin": "miaoxing/admin as 0.12.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 2.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.14.x-dev",
      "miaoxing/services": "miaoxing/services as 0.8.x-dev",
      "miaoxing/app": "miaoxing/app as 0.8.x-dev",
      "miaoxing/logistics": "miaoxing/logistics as 0.5.x-dev",
      "wei/wei": "twinh/wei as 0.17.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^9.1.0"
  }
}
