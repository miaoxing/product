{
  "name": "miaoxing/product",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Product Plugin",
  "keywords": [
    "miaoxing",
    "product"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.12",
    "miaoxing/category": "^0.2.9",
    "miaoxing/admin": "^0.3.10",
    "miaoxing/nav": "^0.1.12",
    "miaoxing/seq": "^0.1.20",
    "miaoxing/logistics": "^0.1.9",
    "miaoxing/ueditor": "^0.1.7"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Product\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Product\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  },
  "require-dev": {
    "miaoxing/dev": "^6.3.3"
  }
}
