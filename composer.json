{
  "name": "miaoxing/product",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Product Plugin",
  "keywords": [
    "miaoxing",
    "product"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.7.7",
    "miaoxing/category": "^0.5.23",
    "miaoxing/admin": "^0.11.1",
    "miaoxing/nav": "^0.1.43",
    "miaoxing/logistics": "^0.5.22",
    "miaoxing/ueditor": "^0.1.37"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Product\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Product\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/admin": "miaoxing/admin as 0.11.x-dev",
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.13.x-dev",
      "miaoxing/services": "miaoxing/services as 0.8.x-dev",
      "miaoxing/app": "miaoxing/app as 0.7.x-dev",
      "miaoxing/logistics": "miaoxing/logistics as 0.5.x-dev",
      "wei/wei": "twinh/wei as 0.15.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.3"
  }
}
