{
  "name": "miaoxing/product",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Product Plugin",
  "keywords": [
    "miaoxing",
    "product"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.3",
    "miaoxing/category": "^0.2.14",
    "miaoxing/admin": "^0.3.15",
    "miaoxing/nav": "^0.1.17",
    "miaoxing/seq": "^0.1.25",
    "miaoxing/logistics": "^0.1.14",
    "miaoxing/ueditor": "^0.1.12"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Product\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Product\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.4.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^7.0.1"
  }
}
